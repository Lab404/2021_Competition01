
@{
    ViewBag.Title = "Create";
}
@ViewBag.err
<h2>Create</h2>
<form action="@Url.Action("Create")" method="post">
    帳號:<input type="text" name="account" id="account" class="form-control"><br>
    密碼:<input type="password" name="password" id="password" class="form-control"><br>
    姓名:<input type="text" name="name" id="name" class="form-control" ><br>
    性別:<br><input type="radio" name="gender" value="0">男
    <input type="radio" name="gender" value="1">女<br>
    Email:<input type="text" name="email" id="email" class="form-control" ><br>
    生日:<input type="text" name="birthday" id="birthday" class="form-control"><br>
    社工編號:<input type="text" name="IDnumber" id="IDnumber" class="form-control"><br>
    驗證碼:<input type="text" name="verification" id="verification" class="form-control"><br>
    <input type="submit" value="送出">
</form>
